{
  "kind": "collectionType",
  "collectionName": "students",
  "info": {
    "name": "Student",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "name": {
      "type": "string",
      "regex": "^([A-Za-z]+)\\s*([A-Za-z]*)\\s+([A-Za-z])\\.$"
    },
    "character": {
      "type": "string"
    },
    "classroom": {
      "via": "students",
      "model": "classroom"
    },
    "sessions": {
      "via": "students",
      "collection": "session",
      "dominant": true
    },
    "enrolled": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "last_logged_in": {
      "type": "datetime"
    },
    "recordings": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    }
  }
}
