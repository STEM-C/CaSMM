{
  "kind": "collectionType",
  "collectionName": "submissions",
  "info": {
    "name": "Submission"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "status": {
      "type": "string",
      "default": "WAITING",
      "required": true
    },
    "workspace": {
      "type": "text",
      "required": true
    },
    "success": {
      "type": "boolean",
      "required": false
    },
    "hex": {
      "type": "text"
    },
    "stdout": {
      "type": "text"
    },
    "stderr": {
      "type": "text"
    },
    "board": {
      "type": "string",
      "required": true
    },
    "sketch": {
      "type": "text",
      "required": true
    },
    "sandbox": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "session": {
      "model": "session",
      "via": "submissions"
    },
    "day": {
      "model": "day"
    }
  }
}
